// Threads 

class Mohan extends Thread {
public void run(){
System.out.println("this is the first thread");
}
}

class main {
public static void main (String args[]){
Mohan moh = new Mohan();
Thread t1 = new Thread (moh);
}
}
==================================
  class Variable {
  
 int a = 10; // instance
 static int b= 20; // statci
 
 public static void main (String args[]){
 
 int c = 30; 
 System.out.println(c);
 Variable  var = new Variable ();
  System.out.println(var.a); // instance variable declares when object create , it require reference variable 
   System.out.println(b); // static variables doesnt require any object creation
   
   }
   }
   
